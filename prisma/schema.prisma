generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Organization {
  id                   String                     @id @default(cuid())
  name                 String
  address              String
  latitude             Float
  longitude            Float
  phone                String
  email                String
  employeeCount        Int                        @default(1)
  logoUrl              String?
  createdAt            DateTime                   @default(now())
  updatedAt            DateTime                   @updatedAt
  roomCount            Int                        @default(1)
  openingDays          String[]                   @default(["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"])
  activityLogs         ActivityLog[]              @relation("ActivityLogOrganization")
  chatRooms            ChatRoom[]
  clinicSchedules      ClinicSchedule[]
  contactPersons       ContactPerson[]
  equipment            Equipment[]
  itemCategories       ItemCategory[]
  leaveRequests        LeaveRequest[]
  locations            Location[]
  managerPersonalNotes ManagerPersonalNotes[]     @relation("ManagerPersonalNotes")
  orders               Order[]
  orderRequests        OrderRequest[]
  themeSettings        OrganizationThemeSettings?
  patients             Patient[]
  products             Product[]
  repairRequests       RepairRequest[]
  scheduleRules        ScheduleRule[]
  tasks                Task[]
  taskBoards           TaskBoard[]
  users                User[]
  vendors              Vendor[]
  fluorideFlavors      FluorideFlavor[]
}

model User {
  id                         String                 @id @default(cuid())
  firstName                  String
  lastName                   String
  email                      String                 @unique
  password                   String
  phone                      String
  address                    String
  role                       UserRole
  organizationId             String?
  createdAt                  DateTime               @default(now())
  updatedAt                  DateTime               @updatedAt
  disabledAt                 DateTime?
  disabledBy                 String?
  disabledReason             String?
  isActive                   Boolean                @default(true)
  isDisabled                 Boolean                @default(false)
  lastLoginAt                DateTime?
  activityLogs               ActivityLog[]          @relation("ActivityLogUser")
  appointments               Appointment[]
  addedBoardMembers          BoardMember[]          @relation("BoardMemberAdder")
  boardMemberships           BoardMember[]          @relation("BoardMember")
  calendarSettings           CalendarSettings?
  chatRooms                  ChatParticipant[]
  dentalProcedures           DentalProcedure[]
  financialReports           FinancialReport[]      @relation("UserFinancialReport")
  reviewedLeaveRequests      LeaveRequest[]         @relation("LeaveRequestReviewer")
  leaveRequests              LeaveRequest[]         @relation("LeaveRequestUser")
  managerPersonalNotes       ManagerPersonalNotes[] @relation("ManagerPersonalNotes")
  messages                   Message[]
  notes                      Note[]
  ordersApproved             Order[]                @relation("ApprovedBy")
  ordersCreated              Order[]                @relation("OrderedBy")
  processedRequests          OrderRequest[]         @relation("ProcessedBy")
  requestedItems             OrderRequest[]         @relation("RequestedBy")
  orderTimelineEventsCreated OrderTimelineEvent[]   @relation("OrderTimelineEventCreatedBy")
  otherWorkerSchedules       OtherWorkerSchedule[]
  pendingAppointments        PendingAppointment[]
  personalThemeSettings      PersonalThemeSettings?
  repairRequestsCreated      RepairRequest[]        @relation("RepairRequestedBy")
  mainRoomAssignments        RoomAssignment[]       @relation("MainPractitioner")
  sideRoomAssignments        RoomAssignment[]       @relation("SidePractitioner")
  mainRoomShifts             RoomShift[]            @relation("RoomShiftMainPractitioner")
  sideRoomShifts             RoomShift[]            @relation("RoomShiftSidePractitioner")
  scheduleOverrides          ScheduleOverride[]
  completedTasks             Task[]                 @relation("TaskCompleter")
  createdTasks               Task[]                 @relation("TaskCreator")
  assignedTasks              TaskAssignment[]       @relation("TaskAssigner")
  taskAssignments            TaskAssignment[]       @relation("TaskAssignee")
  createdBoards              TaskBoard[]            @relation("BoardCreator")
  taskMessages               TaskMessage[]          @relation("TaskMessageSender")
  taskVotes                  TaskVote[]             @relation("TaskVoter")
  disabledByUser             User?                  @relation("UserDisabledBy", fields: [disabledBy], references: [id])
  disabledUsers              User[]                 @relation("UserDisabledBy")
  organization               Organization?          @relation(fields: [organizationId], references: [id])
  dashboardData              UserDashboard?
  expenses                   UserExpense[]          @relation("UserExpense")
  financeSettings            UserFinanceSettings?
  income                     UserIncome[]           @relation("UserIncome")
  waitingAppointmentsCreated WaitingAppointment[]
  waitingListAsCreator       WaitingListEntry[]     @relation("WaitingListCreator")
  waitingListAsPractitioner  WaitingListEntry[]     @relation("WaitingListPractitioner")

  @@index([organizationId])
  @@index([isActive])
  @@index([isDisabled])
  @@index([role])
}

model Patient {
  id                     String                  @id @default(cuid())
  patientCode            String                  @unique
  familyHeadCode         String?
  familyPosition         Int?
  firstName              String
  lastName               String
  dateOfBirth            DateTime
  gender                 String
  email                  String?
  phone                  String?
  address                Json
  bsn                    String
  country                String
  healthInsurance        Json?
  medicalHistory         Json?
  dentalHistory          Json?
  asaScore               Int?
  ppsScores              Json?
  ppsTreatment           String?
  statusPraesens         Json?
  dentalChart            Json?
  periodontalCharts      Json[]
  recallTerm             Int?
  organizationId         String
  isDisabled             Boolean                 @default(false)
  disabledReason         String?
  disabledAt             DateTime?
  disabledBy             String?
  createdAt              DateTime                @default(now())
  updatedAt              DateTime                @updatedAt
  allowEarlySpotContact  Boolean                 @default(true)
  isLongTermCareAct      Boolean                 @default(false)
  activityLogs           ActivityLog[]           @relation("ActivityLogPatient")
  appointments           Appointment[]
  asaHistory             AsaRecord[]
  carePlan               CarePlan?
  cleaningRecallHistory  CleaningRecallRecord[]
  dentalProcedures       DentalProcedure[]
  files                  File[]
  images                 Image[]
  notes                  Note[]
  noteFolders            NoteFolder[]
  organization           Organization            @relation(fields: [organizationId], references: [id])
  statusHistory          PatientStatusRecord[]
  pendingAppointments    PendingAppointment[]
  ppsHistory             PpsRecord[]
  screeningRecallHistory ScreeningRecallRecord[]
  shopPurchases          ShopPurchase[]
  tasks                  Task[]
  treatments             Treatment[]
  waitingAppointments    WaitingAppointment[]
  waitingListEntries     WaitingListEntry[]

  @@index([organizationId])
  @@index([patientCode])
  @@index([familyHeadCode])
}

model DentalCode {
  id           String            @id @default(cuid())
  code         String            @unique
  description  String
  points       Int?
  rate         Float?
  category     String
  requirements Json
  section      String
  subSection   String
  patientType  String
  createdAt    DateTime          @default(now())
  updatedAt    DateTime          @updatedAt
  procedures   DentalProcedure[]
}

model DentalProcedure {
  id              String         @id @default(cuid())
  patientId       String
  codeId          String
  date            DateTime
  notes           String?
  status          String         @default("PENDING")
  createdAt       DateTime       @default(now())
  updatedAt       DateTime       @updatedAt
  practitionerId  String?
  toothNumber     Int?
  quantity        Int            @default(1)
  invoiceEmail    Boolean        @default(false)
  invoicePrinted  Boolean        @default(false)
  isPaid          Boolean        @default(false)
  paidAt          DateTime?
  paymentAmount   Float?
  paymentMethod   PaymentMethod?
  cost            Float?
  subSurfaces     String[]
  fillingMaterial String?
  code            DentalCode     @relation(fields: [codeId], references: [id])
  patient         Patient        @relation(fields: [patientId], references: [id], onDelete: Cascade)
  practitioner    User?          @relation(fields: [practitionerId], references: [id])

  @@index([patientId])
  @@index([codeId])
  @@index([practitionerId])
  @@unique([patientId, toothNumber, codeId])
}

model Appointment {
  id                  String              @id @default(cuid())
  startTime           DateTime
  endTime             DateTime?
  duration            Int?
  type                String
  status              AppointmentStatus
  notes               String?
  appointmentType     AppointmentTypeEnum @default(REGULAR)
  reservationColor    String?
  isReservation       Boolean             @default(false)
  isFamilyAppointment Boolean             @default(false)
  familyAppointmentId String?
  patientId           String?
  practitionerId      String
  treatmentId         String?
  createdAt           DateTime            @default(now())
  updatedAt           DateTime            @updatedAt
  appointmentStatus   Json?
  activityLogs        ActivityLog[]       @relation("ActivityLogAppointment")
  patient             Patient?            @relation(fields: [patientId], references: [id])
  practitioner        User                @relation(fields: [practitionerId], references: [id])
  treatment           Treatment?          @relation(fields: [treatmentId], references: [id])

  @@index([patientId])
  @@index([practitionerId])
  @@index([treatmentId])
  @@index([familyAppointmentId])
}

model Treatment {
  id           String          @id @default(cuid())
  name         String
  description  String
  cost         Float
  status       TreatmentStatus
  patientId    String
  createdAt    DateTime        @default(now())
  updatedAt    DateTime        @updatedAt
  appointments Appointment[]
  patient      Patient         @relation(fields: [patientId], references: [id])

  @@index([patientId])
}

model Image {
  id          String       @id @default(cuid())
  url         String
  type        ImageType
  side        String?
  toothNumber Int?
  dateTaken   DateTime
  notes       String?
  patientId   String
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
  annotations Annotation[]
  calibration Calibration?
  patient     Patient      @relation(fields: [patientId], references: [id], onDelete: Cascade)
}

model PendingAppointment {
  id             String    @id @default(cuid())
  patientId      String
  practitionerId String?
  startTime      DateTime?
  endTime        DateTime?
  duration       Int
  type           String
  status         String    @default("PENDING")
  notes          String?
  requestedBy    String?
  priority       String    @default("medium")
  createdAt      DateTime  @default(now())
  updatedAt      DateTime  @updatedAt
  patient        Patient   @relation(fields: [patientId], references: [id])
  doctor         User?     @relation(fields: [practitionerId], references: [id])

  @@index([patientId])
  @@index([practitionerId])
}

model ScheduleRule {
  id             String       @id @default(cuid())
  startDate      DateTime
  endDate        DateTime
  repeatType     String
  daysOfWeek     String[]
  organizationId String
  createdAt      DateTime     @default(now())
  updatedAt      DateTime     @updatedAt
  schedule       Json?
  organization   Organization @relation(fields: [organizationId], references: [id])

  @@index([organizationId])
}

model CalendarSettings {
  id          String   @id @default(cuid())
  userId      String   @unique
  color       String   @default("#cfdbff")
  visibleDays String[] @default(["monday", "tuesday", "wednesday", "thursday", "friday", "saturday", "sunday"])
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  user        User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId])
}

model ChatRoom {
  id             String            @id @default(cuid())
  name           String?
  type           ChatType          @default(PRIVATE)
  organizationId String
  isGlobal       Boolean           @default(false)
  createdAt      DateTime          @default(now())
  updatedAt      DateTime          @updatedAt
  participants   ChatParticipant[]
  organization   Organization      @relation(fields: [organizationId], references: [id])
  messages       Message[]

  @@index([organizationId])
}

model ChatParticipant {
  id         String   @id @default(cuid())
  userId     String
  chatRoomId String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  chatRoom   ChatRoom @relation(fields: [chatRoomId], references: [id], onDelete: Cascade)
  user       User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, chatRoomId])
  @@index([userId])
  @@index([chatRoomId])
}

model Message {
  id         String      @id @default(cuid())
  content    String
  type       MessageType @default(TEXT)
  fileUrl    String?
  fileName   String?
  chatRoomId String
  senderId   String
  createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
  deletedAt  DateTime?
  chatRoom   ChatRoom    @relation(fields: [chatRoomId], references: [id], onDelete: Cascade)
  sender     User        @relation(fields: [senderId], references: [id], onDelete: Cascade)

  @@index([chatRoomId])
  @@index([senderId])
}

model File {
  id        String   @id @default(cuid())
  name      String
  url       String
  type      FileType
  size      Int
  patientId String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  patient   Patient  @relation(fields: [patientId], references: [id])

  @@index([patientId])
}

model AsaRecord {
  id        String   @id @default(cuid())
  score     Int
  notes     String?
  date      DateTime @default(now())
  patientId String
  createdBy String
  patient   Patient  @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@index([patientId])
}

model PpsRecord {
  id        String       @id @default(cuid())
  quadrant1 Int
  quadrant2 Int
  quadrant3 Int
  quadrant4 Int
  treatment PPSTreatment
  notes     String?
  date      DateTime     @default(now())
  patientId String
  createdBy String
  patient   Patient      @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@index([patientId])
}

model ScreeningRecallRecord {
  id              String   @id @default(cuid())
  screeningMonths Int
  notes           String?
  date            DateTime @default(now())
  patientId       String
  createdBy       String
  patient         Patient  @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@index([patientId])
}

model CleaningRecallRecord {
  id             String   @id @default(cuid())
  cleaningMonths Int
  procedureCode  String
  notes          String?
  date           DateTime @default(now())
  patientId      String
  createdBy      String
  patient        Patient  @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@index([patientId])
}

model CarePlan {
  id          String   @id @default(cuid())
  careRequest String
  careGoal    String
  policy      String
  riskProfile Json
  updatedAt   DateTime @default(now())
  createdBy   String
  patientId   String   @unique
  patient     Patient  @relation(fields: [patientId], references: [id], onDelete: Cascade)
}

model NoteFolder {
  id        String   @id @default(cuid())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  patientId String
  notes     Note[]
  patient   Patient  @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@index([patientId])
}

model Note {
  id        String      @id @default(cuid())
  content   String
  isPinned  Boolean     @default(false)
  pinOrder  Int?
  createdAt DateTime    @default(now())
  updatedAt DateTime    @updatedAt
  createdBy String
  folderId  String?
  patientId String
  user      User        @relation(fields: [createdBy], references: [id])
  folder    NoteFolder? @relation(fields: [folderId], references: [id], onDelete: Cascade)
  patient   Patient     @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@index([patientId])
  @@index([folderId])
  @@index([createdBy])
}

model PatientStatusRecord {
  id        String   @id @default(cuid())
  patientId String
  action    String
  reason    String?
  date      DateTime @default(now())
  createdBy String
  patient   Patient  @relation(fields: [patientId], references: [id], onDelete: Cascade)

  @@index([patientId])
  @@index([createdBy])
}

model Calibration {
  id          String   @id @default(cuid())
  pixelWidth  Float
  pixelHeight Float
  realWidth   Float
  realHeight  Float
  unit        String
  imageId     String   @unique
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  image       Image    @relation(fields: [imageId], references: [id], onDelete: Cascade)
}

model Annotation {
  id          String   @id @default(cuid())
  type        String
  points      Json
  text        String?
  color       String
  size        Int
  measurement Float?
  imageId     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  image       Image    @relation(fields: [imageId], references: [id], onDelete: Cascade)
}

model OrganizationThemeSettings {
  id                  String       @id @default(cuid())
  organizationId      String       @unique
  primaryColor        String       @default("#3b82f6")
  primaryForeground   String       @default("#ffffff")
  secondaryColor      String       @default("#f1f5f9")
  secondaryForeground String       @default("#0f172a")
  accentColor         String       @default("#10b981")
  accentForeground    String       @default("#ffffff")
  backgroundColor     String       @default("#ffffff")
  surfaceColor        String       @default("#f8fafc")
  borderColor         String       @default("#e2e8f0")
  textPrimary         String       @default("#0f172a")
  textSecondary       String       @default("#64748b")
  textMuted           String       @default("#94a3b8")
  successColor        String       @default("#10b981")
  warningColor        String       @default("#f59e0b")
  errorColor          String       @default("#ef4444")
  infoColor           String       @default("#3b82f6")
  fontFamily          String       @default("Inter")
  headingFontFamily   String       @default("Inter")
  fontSize            String       @default("14")
  headingScale        String       @default("1.25")
  lineHeight          String       @default("1.5")
  letterSpacing       String       @default("0")
  borderRadius        String       @default("6")
  spacing             String       @default("1")
  maxWidth            String       @default("1200")
  sidebarWidth        String       @default("280")
  buttonSize          String       @default("md")
  inputSize           String       @default("md")
  avatarSize          String       @default("md")
  iconSize            String       @default("20")
  shadowLevel         String       @default("md")
  animationSpeed      String       @default("200")
  calendarTodayBg     String       @default("#ddd6fe")
  calendarAccentBg    String       @default("#f3f4f6")
  customVariables     Json?
  createdAt           DateTime     @default(now())
  updatedAt           DateTime     @updatedAt
  organization        Organization @relation(fields: [organizationId], references: [id], onDelete: Cascade)

  @@index([organizationId])
}

model PersonalThemeSettings {
  id                  String   @id @default(cuid())
  userId              String   @unique
  primaryColor        String   @default("#3b82f6")
  primaryForeground   String   @default("#ffffff")
  secondaryColor      String   @default("#f1f5f9")
  secondaryForeground String   @default("#0f172a")
  accentColor         String   @default("#10b981")
  accentForeground    String   @default("#ffffff")
  backgroundColor     String   @default("#ffffff")
  surfaceColor        String   @default("#f8fafc")
  borderColor         String   @default("#e2e8f0")
  textPrimary         String   @default("#0f172a")
  textSecondary       String   @default("#64748b")
  textMuted           String   @default("#94a3b8")
  successColor        String   @default("#10b981")
  warningColor        String   @default("#f59e0b")
  errorColor          String   @default("#ef4444")
  infoColor           String   @default("#3b82f6")
  fontFamily          String   @default("Inter")
  headingFontFamily   String   @default("Inter")
  fontSize            String   @default("14")
  headingScale        String   @default("1.25")
  lineHeight          String   @default("1.5")
  letterSpacing       String   @default("0")
  borderRadius        String   @default("6")
  spacing             String   @default("1")
  maxWidth            String   @default("1200")
  sidebarWidth        String   @default("280")
  buttonSize          String   @default("md")
  inputSize           String   @default("md")
  avatarSize          String   @default("md")
  iconSize            String   @default("20")
  shadowLevel         String   @default("md")
  animationSpeed      String   @default("200")
  calendarTodayBg     String   @default("#ddd6fe")
  calendarAccentBg    String   @default("#f3f4f6")
  customVariables     Json?
  createdAt           DateTime @default(now())
  updatedAt           DateTime @updatedAt
  user                User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId])
}

model UserDashboard {
  id         String   @id @default(cuid())
  userId     String   @unique
  quickNote  String?
  quickLinks Json?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  user       User     @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model Task {
  id             String           @id @default(cuid())
  title          String
  description    String?
  status         TaskStatus       @default(PENDING)
  priority       TaskPriority     @default(MEDIUM)
  deadline       DateTime?
  patientId      String?
  createdBy      String
  organizationId String
  createdAt      DateTime         @default(now())
  updatedAt      DateTime         @updatedAt
  completedAt    DateTime?
  completedBy    String?
  boardId        String?
  type           TaskType         @default(TASK)
  visibility     TaskVisibility   @default(PRIVATE)
  activityLogs   ActivityLog[]    @relation("ActivityLogTask")
  board          TaskBoard?       @relation(fields: [boardId], references: [id])
  completer      User?            @relation("TaskCompleter", fields: [completedBy], references: [id])
  creator        User             @relation("TaskCreator", fields: [createdBy], references: [id])
  organization   Organization     @relation(fields: [organizationId], references: [id])
  patient        Patient?         @relation(fields: [patientId], references: [id], onDelete: Cascade)
  assignments    TaskAssignment[]
  messages       TaskMessage[]
  options        TaskOption[]
  reminders      TaskReminder[]
  votes          TaskVote[]

  @@index([patientId])
  @@index([boardId])
  @@index([createdBy])
  @@index([organizationId])
  @@index([status])
  @@index([deadline])
  @@index([visibility])
  @@index([type])
}

model TaskBoard {
  id             String        @id @default(cuid())
  name           String
  description    String?
  color          String?
  isPublic       Boolean       @default(false)
  createdBy      String
  organizationId String
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt
  members        BoardMember[]
  tasks          Task[]
  creator        User          @relation("BoardCreator", fields: [createdBy], references: [id])
  organization   Organization  @relation(fields: [organizationId], references: [id])

  @@index([createdBy])
  @@index([organizationId])
  @@index([isPublic])
}

model BoardMember {
  id        String    @id @default(cuid())
  boardId   String
  userId    String
  role      BoardRole @default(MEMBER)
  addedBy   String
  createdAt DateTime  @default(now())
  adder     User      @relation("BoardMemberAdder", fields: [addedBy], references: [id])
  board     TaskBoard @relation(fields: [boardId], references: [id], onDelete: Cascade)
  user      User      @relation("BoardMember", fields: [userId], references: [id])

  @@unique([boardId, userId])
  @@index([boardId])
  @@index([userId])
}

model TaskOption {
  id        String     @id @default(cuid())
  taskId    String
  text      String
  order     Int        @default(0)
  createdAt DateTime   @default(now())
  task      Task       @relation(fields: [taskId], references: [id], onDelete: Cascade)
  votes     TaskVote[]

  @@index([taskId])
}

model TaskVote {
  id        String      @id @default(cuid())
  taskId    String
  optionId  String?
  userId    String
  createdAt DateTime    @default(now())
  updatedAt DateTime    @updatedAt
  option    TaskOption? @relation(fields: [optionId], references: [id], onDelete: Cascade)
  task      Task        @relation(fields: [taskId], references: [id], onDelete: Cascade)
  user      User        @relation("TaskVoter", fields: [userId], references: [id])

  @@unique([taskId, userId])
  @@index([taskId])
  @@index([optionId])
  @@index([userId])
}

model TaskAssignment {
  id             String   @id @default(cuid())
  taskId         String
  userId         String
  assignedBy     String
  createdAt      DateTime @default(now())
  isSelfAssigned Boolean  @default(false)
  assigner       User     @relation("TaskAssigner", fields: [assignedBy], references: [id])
  task           Task     @relation(fields: [taskId], references: [id], onDelete: Cascade)
  user           User     @relation("TaskAssignee", fields: [userId], references: [id])

  @@unique([taskId, userId])
  @@index([taskId])
  @@index([userId])
}

model TaskMessage {
  id        String   @id @default(cuid())
  taskId    String
  senderId  String
  content   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  sender    User     @relation("TaskMessageSender", fields: [senderId], references: [id])
  task      Task     @relation(fields: [taskId], references: [id], onDelete: Cascade)

  @@index([taskId])
  @@index([senderId])
}

model TaskReminder {
  id           String    @id @default(cuid())
  taskId       String
  reminderTime DateTime
  message      String?
  sent         Boolean   @default(false)
  sentAt       DateTime?
  createdAt    DateTime  @default(now())
  task         Task      @relation(fields: [taskId], references: [id], onDelete: Cascade)

  @@index([taskId])
  @@index([reminderTime])
}

model WaitingListEntry {
  id                   String              @id @default(cuid())
  patientId            String
  practitionerId       String
  notes                String?
  createdAt            DateTime            @default(now())
  updatedAt            DateTime            @updatedAt
  createdBy            String
  waitingAppointmentId String?             @unique
  status               WaitingListStatus   @default(ACTIVE)
  creator              User                @relation("WaitingListCreator", fields: [createdBy], references: [id])
